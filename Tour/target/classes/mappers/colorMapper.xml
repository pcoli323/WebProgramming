<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.tour.mapper.ColorMapper">
	
	<select id="color" resultType="string">
		select color
		from tbl_Color
		where colorNumber = #{colorNumber}
	</select>
	
	<select id="colors" resultType="string">
		select color
		from tbl_Color
	</select>
	
</mapper>