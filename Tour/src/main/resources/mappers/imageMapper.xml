<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.mapper.ImageMapper">
	
	<insert id="create">
 		insert into tbl_image (title, realfile) 
 		values(#{title},#{realfile})
 	</insert>
	
	<select id="getImage" resultType="object">
		select realfile
		from tbl_image
		where title = #{title}
	</select>
</mapper>